(define zero (lambda (f) (lambda (x) x)))
(define (add-1 n)
  (lambda (f) (lambda (x) (f ((n f) x)))))
(define one (add-1 zero))
(define two (add-1 one))
(define (+ a b)
  (lambda (f) (lambda (x)
                ((a f) ((b f) x)))))
(define (* a b)
  (lambda (f) (lambda (x)
                ((a (b f)) x))))
